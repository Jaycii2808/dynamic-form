[
   {
      "id": "file_uploader_1",
      "type": "fileUploaderFormType",
      "config": {
         "title": "Drop files or click to upload",
         "button_text": "Browse",
         "allowed_extensions": ["jpg", "png", "pdf"]
      },
      "style": {
         "width": 320,
         "height": 200,
         "background_color": "#000000",
         "border_color": "#4A4A4A",
         "border_radius": 12,
         "border_width": 1.5,
         "border_style": "dashed",
         "text_color": "#FFFFFF",
         "button_background_color": "#6979F8",
         "button_text_color": "#FFFFFF",
         "button_border_radius": 8,
         "margin": "16"
      },
      "variants": {
         "dragging": {
            "config": {
               "title": "Drop files here",
               "subtitle": "or"
            },
            "style": {
               "background_color": "#1A1A1A",
               "border_color": "#6979F8"
            }
         }
      },
      "states": {
         "loading": {
            "config": {
               "status_text_format": "Uploading {fileName} {progress}/{total}%",
               "subtitle": "very soon it's over",
               "button_text": "Loading"
            },
            "style": {
               "icon": "file",
               "icon_color": "#FFFFFF",
               "progress_color": "#6979F8"
            }
         },
         "success": {
            "config": {
               "status_text_format": "{fileName} uploaded!",
               "button_text": "Remove"
            },
            "style": {
               "icon": "check",
               "icon_color": "#27AE60"
            }
         },
         "error": {
            "config": {
               "status_text": "Upload failed. Please try again.",
               "button_text": "Retry"
            },
            "style": {
               "icon": "error",
               "icon_color": "#E74C3C",
               "border_color": "#E74C3C"
            }
         }
      }
   },
   {
